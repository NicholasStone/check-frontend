{
  "name": "model-checker",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "electron .",
    "preview": "vite preview",
    "electron:build": "electron-packager . checker --platform=darwin --electron-version=25.3.1 --overwrite --download.mirrorOptions.mirror=https://npmmirror.com/mirrors/electron/ --out=./out ",
    "electron:win7-build": "electron-packager . checker --platform=win32 --arch=ia32 --electron-version=25.3.1 --overwrite --download.mirrorOptions.mirror=https://npmmirror.com/mirrors/electron/ --out=./out --asar",
    "electron22:win7-build": "electron-packager . checker --platform=win32 --arch=ia32 --electron-version=22.3.26 --overwrite --download.mirrorOptions.mirror=https://npmmirror.com/mirrors/electron/ --out=./out --asar",
    "electron22:win7-build2": "electron-packager . checker --platform=win32 --arch=ia32 --electron-version=22.3.26 --overwrite --download.mirrorOptions.mirror=https://npmmirror.com/mirrors/electron/ --out=./out",
    "electron:win10-build": "electron-packager . checker --platform=win32 --arch=x64 --electron-version=25.3.1 --overwrite --download.mirrorOptions.mirror=https://npmmirror.com/mirrors/electron/ --out=./out "
  },
  "dependencies": {
    "@ant-design/colors": "^8.0.0",
    "@ant-design/fast-color": "^3.0.0",
    "@ant-design/icons": "^5.3.0",
    "@ant-design/icons-svg": "^4.4.2",
    "@babel/runtime": "^7.27.0",
    "@monaco-editor/react": "^4.6.0",
    "@reduxjs/toolkit": "^2.2.1",
    "antd": "^5.21.0",
    "axios": "^1.6.7",
    "cnpm": "^9.4.0",
    "dmg-builder": "^25.1.8",
    "electron-builder-squirrel-windows": "^25.1.8",
    "electron-squirrel-startup": "^1.0.1",
    "http-proxy": "^1.18.1",
    "http-server": "^14.1.1",
    "konva": "^9.3.6",
    "monaco-editor": "^0.25.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-konva": "^18.2.10",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.2",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-dmg": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@electron/packager": "^18.3.6",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "latest",
    "electron-builder": "^25.1.8",
    "eslint": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-react-refresh": "latest",
    "json-server": "^1.0.0-alpha.23",
    "vite": "latest",
    "wait-on": "^8.0.1"
  },
  "build": {
    "appId": "top.boywei.model-checker",
    "productName": "model-checker",
    "executableName": "model-checker",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "./dist"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "description": "an integrated tool environment for modeling, simulation and verification of real-time systems",
  "main": "main.js",
  "homepage": ".",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
